{
	"name": "publisher-dci",
	"description": "Publishes things",
	"json_class": "Chef::Role",
	"chef_type": "role",
	"run_list": [
		"role[server-common]",
		"recipe[publisher]"
	],

	"override_attributes": {
		"aptly": {
			"address": "dci.ds9.pub",
			"apiport": 8081,
			"user": "dci",
			"dependencyfollowrecommends": true,
			"keyserver": "keyserver.ubuntu.com",
			"architectures": ["amd64", "armhf", "i386", "all", "source", "arm64"]
			}
		},

		"apache": {
			"redirects": []
		}
	}
}