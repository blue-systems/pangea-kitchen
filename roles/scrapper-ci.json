{
  "name": "scrapper-ci",
  "description": "",
  "json_class": "Chef::Role",
  "default_attributes": {
    "docker" : {
      "install_type": "package",
      "user": "docker",
      "group": "docker",
      "group_members": "jenkins"
    },
    "jenkins-master-apache" : {
      "server_port": "8080",
      "server_name": "build.neon.kde.org.uk",
      "server_aliases": ["build.neon.kde.org.uk", "neon.pangea.pub"]
    },
    "rvm": {
      "user_installs": [
        {
          "user": "jenkins",
          "default_ruby": "ruby-2.2.3",
          "rubies": ["2.2.3"],
          "home": "/var/lib/jenkins"
        }
      ]
    }
  },
  "override_attributes": {
  },
  "chef_type": "role",
  "run_list": [
    "role[jenkins-master]",
    "recipe[jenkins-master-apache]",
    "recipe[docker]",
    "recipe[rvm::user]",
    "recipe[publisher-nci]",
    "recipe[images-nci]"
  ],
  "env_run_lists": {

  }
}
